generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Utsuwa {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  url       String
  content   String?
  loadCount Int      @default(0)
}

